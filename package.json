{
  "name": "@openfort/openfort-node",
  "version": "0.5.5",
  "description": "OpenAPI client for Openfort API",
  "author": "Openfort",
  "repository": {
    "type": "git",
    "url": "https://github.com/openfort-xyz/openfort-node.git"
  },
  "keywords": [
    "fetch",
    "typescript",
    "openapi-client",
    "openapi-generator"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*"
  },
  "typesVersions": {
    "*": {
      "model/*": [
        "dist/model/*"
      ],
      "api/*": [
        "dist/api/*"
      ]
    }
  },
  "typings": "./dist/index.d.ts",
  "scripts": {
    "prebuild": "node -p \"'export const VERSION = ' + JSON.stringify(require('./package.json').version) + ';\\nexport const PACKAGE = ' + JSON.stringify(require('./package.json').name) + ';'\" > src/version.ts",
    "build": "tsc",
    "format": "rome format . --write",
    "lint": "rome check .",
    "lint:fix": "rome check . --apply-unsafe",
    "prepublishOnly": "yarn install && yarn build"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "request": "^2.81.0",
    "rewire": "^3.0.2"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.33",
    "@types/node": "^12",
    "@types/request": "^2.48.8",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "rome": "^12.1.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
